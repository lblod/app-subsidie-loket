DELETE {
  GRAPH ?g {
    ?s dct:publisher <http://data.lblod.info/services/loket-producer>.
  }
}
INSERT {
  GRAPH ?g {
    ?s dct:publisher <http://data.lblod.info/services/delta-producer-publication-graph-maintainer-submissions>.
  }
}
WHERE {

  GRAPH ?g {
    ?s dct:publisher <http://data.lblod.info/services/loket-producer>.
  }
   ?source nie:dataSource ?s.

  FILTER (regex(str(?source), 'submissions'))
}